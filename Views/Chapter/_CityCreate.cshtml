@model TomorrowsVoice_Toplevel.Models.City
<form asp-controller="City" asp-action="Create" method="post" id="createCityForm">
	<div class="row g-2">
		<div class="text-danger validation-summary-errors"><ul id="ErrorList"></ul></div>
		<div class="col-md-6">
			<div class="form-group">
				<label asp-for="Name" class="control-label"></label>
				<input asp-for="Name" class="form-control" />
				<span asp-validation-for="Name" class="text-danger"></span>
			</div>
		</div>
		<div class="col-md-6">
			<div class="form-group">
				<label asp-for="Province" class="control-label"></label>
				<select asp-for="Province" class="form-control"
						asp-items="Html.GetEnumSelectList<Province>()"></select>
				<span asp-validation-for="Province" class="text-danger"></span>
			</div>
		</div>
			<div class="form-group">
				<input type="submit" id="submitCityCreate" value="Save" class="btn btn-outline-dark" />
			</div>
		
	</div>
</form>